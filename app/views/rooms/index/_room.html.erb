<%= turbo_frame_tag dom_id(room), class: "flex #{"border-b border-solid border-gray-200" unless room_iteration.last?} py-4" do %>
  <h2 class="flex-1"><%= room.title %></h2>
  <%= link_to "Enter", room_path(room), class: "px-2", data: { turbo_frame: "_top" } %>
  <%= link_to "Edit", edit_room_path(room), class: "px-2" %>
  <%= link_to "Delete", room_path(room), method: :delete, class: "px-2" %>
<% end %>
